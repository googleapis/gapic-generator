interfaces:
- name: google.example.library.v1.LibraryService
  collections:
  - name_pattern: /shelves/{shelf}
    entity_name: shelf
  - name_pattern: /shelves/{shelf}/books/{book}
    entity_name: book
  methods:
  - name: CreateShelf
    flattening:
      groups:
      - parameters:
        - shelf
  - name: GetShelf
    flattening:
      groups:
      - parameters:
        - name
        - message
        - string_builder
  - name: ListShelves
    page_streaming:
      request:
        token_field: page_token
      response:
        token_field: next_page_token
        resource_field: shelves
  - name: DeleteShelf
    flattening:
      groups:
      - parameters:
        - name
  - name: MergeShelves
    flattening:
      groups:
      - parameters:
        - name
        - other_shelf_name
  - name: CreateBook
    flattening:
      groups:
      - parameters:
        - name
        - book
  - name: PublishSeries
    flattening:
      groups:
      - parameters:
        - shelf
        - books
        - edition
  - name: GetBook
    flattening:
      groups:
      - parameters:
        - name
  - name: ListBooks
    flattening:
      groups:
      - parameters:
        - name
        - filter
    page_streaming:
      request:
        token_field: page_token
      response:
        token_field: next_page_token
        resource_field: books
  - name: DeleteBook
    flattening:
      groups:
      - parameters:
        - name
  - name: UpdateBook
  - name: MoveBook
    flattening:
      groups:
      - parameters:
        - name
        - other_shelf_name
  - name: ListStrings
    flattening:
      groups:
      - parameters:
        - name
    page_streaming:
      request:
        token_field: page_token
      response:
        token_field: next_page_token
        resource_field: strings
  - name: AddComments
    flattening:
      groups:
      - parameters:
        - name
        - comments
