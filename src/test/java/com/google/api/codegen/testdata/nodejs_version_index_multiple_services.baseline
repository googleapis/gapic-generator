'use strict';

var extend = require('extend');
var gapic = {
  v1: require('./v1')
};
var gaxGrpc = require('google-gax').grpc();

const VERSION = require('../package.json').version;

/**
 * Create an incrementerServiceClient with additional helpers for common
 * tasks.
 *

 *
 * @param {object=} options - [Configuration object](#/docs).
 * @param {string=} options.key - API key to use form communication with the
 *     service.
 * @param {string=} options.keyFilename - Path to a key file to use for an
 *     API key.
 * @param {(string|number)=} options.logLevel - An integer from 0-5
 *     inclusive or a decimal representation of an integer from 0-5 inclusive
 *     in String form.
 * @param {number=} options.port - The port on which to connect to
 *     the remote host.
 * @param {string=} options.projectId - The projectId of the project
 *     deployed.
 * @param {object=} options.serviceContext - The service context of the
 *     application.
 * @param {string=} options.serviceContext.service - The service the
 *     application is running on.
 * @param {string=} options.serviceContext.version - The version the hosting
 *     application is currently labelled as.
 * @param {string=} options.servicePath - The domain name of the
 *     API remote host.
 * @param {uncaughtHandlingEnum}
 *     [onCaughtException=uncaughtHandlingEnum.ignore] - One of the uncaught
 *     handling options.
 */
function incrementerV1(options) {
  // Define the header options.
  options = extend({}, options, {
    libName: 'gccl',
    libVersion: VERSION
  });

  // Create the client with the provided options.
  var client = gapic.v1(options).incrementerServiceClient(options);
  return client;
}

