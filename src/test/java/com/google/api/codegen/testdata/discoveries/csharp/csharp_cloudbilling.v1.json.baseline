using Google.Apis.Auth.OAuth2;
using Google.Apis.Cloudbilling.v1;
using Google.Apis.Services;
using System.Threading.Tasks;

using Data = Google.Apis.Cloudbilling.v1.Data;

namespace CloudbillingSample
{
