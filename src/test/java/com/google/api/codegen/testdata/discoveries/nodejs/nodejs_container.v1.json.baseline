    // The name of the Google Compute Engine [zone](/compute/docs/zones#available) in which the cluster
    // resides.
    zone: "",
    resource: {},
    // Auth client
    auth: authClient
  };

  container.projects.zones.clusters.create(request, function(err, result) {
    if (err) {
      console.log(err);
    } else {
      console.log(result);
    }
  });
});
