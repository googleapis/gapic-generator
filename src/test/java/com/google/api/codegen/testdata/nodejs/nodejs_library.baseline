    protoFilesRoot = protobuf.loadSync(
      path.join(__dirname, '..', '..', 'protos', 'library.proto'),
      protoFilesRoot
    );
    protoFilesRoot = protobuf.loadSync(
      path.join(__dirname, '..', '..', 'protos', 'tagger.proto'),
      protoFilesRoot
