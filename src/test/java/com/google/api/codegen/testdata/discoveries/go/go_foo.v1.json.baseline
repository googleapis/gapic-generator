import (
  "fmt"
  "log"
  "net/http"

  "golang.org/x/net/context"
  "google.golang.org/api/foo/v1"
)

func main() {
  ctx := context.Background()

  c := &http.Client{}

  fooService, err := foo.New(c)
  if err != nil {
    log.Fatal(err)
  }

  resp, err := fooService.Baz.Get().Context(ctx).Do()
  if err != nil {
    log.Fatal(err)
  }

  // TODO: Change code below to process the `resp` object:
  fmt.Printf("%#v\n", resp)
}
