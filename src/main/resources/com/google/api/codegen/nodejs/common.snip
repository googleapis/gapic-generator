@extends "common.snip"

@snippet comments(commentLines)
  @join comment : commentLines
    * {@comment}
  @end
@end

@snippet lineComments(commentLines)
  @join comment : commentLines
    // {@comment}
  @end
@end

@snippet importList(fileImports)
	@join import : fileImports
		@if import.moduleName
			const {@import.moduleName} = require('{@import.moduleName}');
		@end
	@end
@end

@snippet if(c, t, f)
  @if c
    {@t}
  @else
    {@f}
  @end
@end