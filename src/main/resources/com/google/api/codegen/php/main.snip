@snippet generateFilename(service)
    {@context.getApiWrapperName(service)}.php
@end

@snippet generateClass(service, body, imports)
    <?php
    {@licenseSection(service)}

    namespace {@context.getApiConfig.getPackageName};

    @join import : imports
        use {@import};
    @end

    {@body}
@end

